{
  "name": "ta-math",
  "version": "0.1.0",
  "description": "Technical analysis library in javascript. Calculates indicators and overlays from ohlcv data",
  "main": "ta-math.js",
  "scripts": {
    "build": "rollup ./src/main.mjs -f cjs > ./ta-math.js && rollup ./test/smoke.mjs -f cjs > ./test.js",
    "test": "node ./test.js",
    "travis": "npm run build && npm run test",
    "view": "start ./examples/page.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/munrocket/ta-math.git"
  },
  "keywords": [
    "technical-analysis",
    "quantitative-finance",
    "indicators",
    "overlays",
    "finance",
    "ohlcv"
  ],
  "author": "munrocket",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/munrocket/ta-math/issues"
  },
  "homepage": "https://github.com/munrocket/ta-math#readme",
  "dependencies": {},
  "devDependencies": {
    "rollup": "^0.62.0",
    "tape": "^4.9.1"
  }
}
