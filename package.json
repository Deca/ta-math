{
  "name": "ta-math",
  "version": "0.3.2",
  "description": "Technical analysis library in javascript. Calculates indicators and overlays from ohlcv data",
  "main": "dist/ta-math.cjs.js",
  "module": "dist/ta-math.esm.js",
  "browser": "dist/ta-math.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "nyc mocha",
    "test-ui": "start ./examples/example.html",
    "build-test": "npm run build && npm run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munrocket/ta-math.git"
  },
  "keywords": [
    "technical",
    "indicators",
    "overlays",
    "ohlcv",
    "quantitative-finance",
    "technical-analysis",
    "finance",
    "library"
  ],
  "author": "munrocket",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/munrocket/ta-math/issues"
  },
  "homepage": "https://github.com/munrocket/ta-math#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-proxy": "^1.1.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-builtins": "^2.1.2"
  }
}
