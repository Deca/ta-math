{
  "name": "ta-math",
  "version": "0.1.1",
  "description": "Technical analysis library in javascript. Calculates indicators and overlays from ohlcv data",
  "main": "ta-math.js",
  "scripts": {
    "build": "npm run build:node && npm run build:bro",
    "build:node": "rollup src/main.mjs -f cjs > ta-math.js && rollup test/smoke.mjs -f cjs > test.js",
    "build:bro": "browserify ta-math.js > dist/ta-math.browser.js",
    "test": "npm run build && node ./test.js && start ./examples/page.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/munrocket/ta-math.git"
  },
  "keywords": [
    "technical-analysis",
    "quantitative-finance",
    "indicators",
    "overlays",
    "finance",
    "library",
    "ohlcv"
  ],
  "author": "munrocket",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/munrocket/ta-math/issues"
  },
  "homepage": "https://github.com/munrocket/ta-math#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "browserify": "^16.2.2",
    "rollup": "^0.62.0",
    "tape": "^4.9.1"
  }
}
